VERSION = "1.2.0"
STAGE = "rc"
RELEASE_DATE = "2014-07-20"
SVN_REVISION = proc {
                 vers = `svnversion`
                 rev = $?.success? ? vers.strip : 'unknown'
                 if rev == 'exported' 
                   ma = /\$Revision:\s*(\d+)\s*\$/.match("$Revision$")
                   rev = ma ? ma[1] : 'unknown'
                 end
                 rev
               }.call
