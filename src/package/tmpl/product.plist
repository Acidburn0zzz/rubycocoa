<%
  framework = "RubyCocoa"
  version = @config['rubycocoa-version']
  version_short = version[/\d+(?:\.\d+){1,2}/] # x.y.z
  system_version = @config['macosx-deployment-target']
	target_archs = @config['target-archs'].split
 %>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>os</key>
	<array>
		<string><%= system_version %></string>
	</array>
	<key>arch</key>
	<array><% target_archs.each do |arch| %>
		<string><%= arch %></string>
	<% end %></array>
	<key>bundle</key>
	<array>
	</array>
</dict>
</plist>
